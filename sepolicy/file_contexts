# Camera
/data/misc/camera/cam_socket.*                    u:object_r:camera_socket:s0

# GPS
/dev/gss                                          u:object_r:sensors_device:s0

# IRRC
/dev/msm_IRRC_pcm_dec                             u:object_r:irrc_device:s0
/system/bin/irrcServer                            u:object_r:irrcServer_exec:s0

# CMHW
/sys/devices/platform/kcal_ctrl.0/kcal.*          u:object_r:kcal_sysfs:s0
/sys/devices/virtual/input/lge_touch/knock_on     u:object_r:knock_sysfs:s0
/sys/class/timed_output/vibrator/amp              u:object_r:vibeamp_sysfs:s0
